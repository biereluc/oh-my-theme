{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "p:dark-color",
    "dark-color": "#003543",
    "foreground": "p:light-color",
    "light-color": "#9affdc",
    "moon-light": "#90A4AE",
    "sun-light": "#FFC300",
    "warning-red": "#ff6b6b"
  },
  "var": {
    "ShowPromptLineCount": "yes",
    "AdminIcon": "\uf0e7"
  },
  "palettes": {
    "list": {
      "Goodmorningsunshine": {
        "background": "p:dark-color",
        "dark-color": "#003543",
        "foreground": "p:light-color",
        "light-color": "#9affdc",
        "moon-light": "#90A4AE",
        "sun-light": "#FFC300",
        "warning-red": "#ff6b6b"
      },
      "Halo": {
        "background": "p:dark-color",
        "dark-color": "#2f3e46",
        "foreground": "p:light-color",
        "light-color": "#84a98c",
        "moon-light": "#90A4AE",
        "sun-light": "#FFC300",
        "warning-red": "#ff7f51"
      },
      "RedWine": {
        "dark-color": "#03071e",
        "light-color": "#9d0208",
        "warning-red": "#e85d04"
      },
      "aaa": {
        "background": "p:dark-color",
        "dark-color": "#2b2d42",
        "foreground": "p:light-color",
        "light-color": "#8d99ae",
        "moon-light": "#90A4AE",
        "sun-light": "#FFC300",
        "warning-red": "#d90429"
      },
      "midnight": {
        "background": "p:dark-color",
        "dark-color": "#003543",
        "foreground": "p:light-color",
        "light-color": "#9affdc",
        "moon-light": "#90A4AE",
        "sun-light": "#FFC300",
        "warning-red": "#ff6b6b"
      }
    },
    "template": "{{- $theme := (split \"|\" \"RedWine|Goodmorningsunshine|Halo|midnight|aaa\") -}}{{ $theme }}"
  },
  "valid_line": {
    "template": "\u2500‚ùØ",
    "foreground": "p:light-color",
    "background": "transparent"
  },
  "secondary_prompt": {
    "template": "\uf067\u200a",
    "background": "transparent",
    "foreground_templates": [
      "p:light-color"
    ]
  },
  "transient_prompt": {
    "template": "{{if eq .Var.ShowPromptLineCount \"yes\"}}<p:light-color>{{.PromptCount}}</>\u200a{{end}}\ue285 ",
    "background": "transparent",
    "foreground_templates": [
      "{{ if .Root }}p:warning-red{{ else }}p:light-color{{ end }}"
    ]
  },
  "error_line": {
    "template": "\u200a\ue009‚ùØ",
    "foreground": "p:warning-red",
    "background": "transparent"
  },
  "terminal_background": "darkGray",
  "pwd": "osc51",
  "accent_color": "#003543",
  "console_title_template": "{{if .Root}}üîêAdmin : {{end}}{{.Shell}} in {{.PWD}} {{if .Root}}‚ö°{{end}}",
  "blocks": [
    {
      "type": "prompt",
      "alias": "left_prompt_decorations",
      "alignment": "left",
      "overflow": "break",
      "segments": [
        {
          "type": "text",
          "alias": "shield_admin_icon",
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ if .Root }}\u200a\uf132{{ end }}\u200a",
          "foreground": "p:dark-color",
          "background": "p:dark-color",
          "style": "diamond",
          "background_templates": [
            "{{ if .Root }}p:warning-red{{ end }}"
          ]
        },
        {
          "type": "root",
          "alias": "dummy-admin-segment-to-display-nice-powerline-symbol",
          "properties": {
            "cache_duration": "none"
          },

          "template": "\u200a",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "background": "p:dark-color"

        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}{{ if.Root }}<p:warning-red>\uf0e7</>{{ else }}@{{ end }}{{ .HostName }}\u200a",
          "foreground": "p:light-color",
          "background": "parentBackground",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\u2026",
            "folder_separator_icon": "\u200a<p:dark-color>\ue0b1</>\u200a",
            "home_icon": "\uf7db",
            "interactive": true,
            "mapped_locations": {
              "C:\\Users": "\uf007",
              "C:\\Windows": "\ue62a",
              "C:\\temp": "\ue799"
            },
            "mapped_locations_enabled": true,
            "mixed_threshold": 4,
            "style": "unique"
          },
          "template": "\u200a{{ if .Writable }}{{else}}<p:warning-red>\uf023\u200a{{end}}{{ path .Path .Location}}</>\u200a",
          "foreground": "p:dark-color",
          "powerline_symbol": "\ue0b0",
          "background": "p:light-color",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 1
          },
          "template": "\u200a{{ if ne .FormattedMs \"0ms\" }}\ufbab{{ else }}<p:warning-red>\ufbab</>{{ end }}\u200a{{ .FormattedMs }}\u200a",
          "foreground": "p:light-color",
          "powerline_symbol": "\ue0b0",
          "background": "p:dark-color",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "\u200a{{ if gt .Code 0 }}\uf00d\u200a{{ .Meaning }}{{ else }}\uf42e{{ end }}",
          "foreground": "p:dark-color",
          "type": "exit",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}p:warning-red{{ else }}p:light-color{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alias": "right_prompt_decorations",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "shell",
          "alias": "shell_name",
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "Bash",
              "powershell": "PS",
              "pwsh": "PWSH"
            }
          },
          "leading_diamond": "\ue0b6",
          "template": "\uf489\u200a{{ .Name }}\u200a",
          "foreground": "p:light-color",
          "background": "p:dark-color",
          "style": "diamond"
        },
        {
          "type": "os",
          "alias": "os_icon",
          "properties": {
            "cache_duration": "none",
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "template": "\u200a{{ if .WSL }}p:warning-red\ue712{{ end }}{{ .Icon }}",
          "foreground": "p:dark-color",
          "powerline_symbol": "<r>\ue0b2</r>",
          "background": "p:light-color",
          "style": "powerline"
        },
        {
          "type": "battery",
          "alias": "battery_percentage",
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf581",
            "charging_icon": "\ufba3",
            "discharging_icon": "\uf579",
            "display_error": false,
            "not_charging_icon": "\uf582"
          },
          "template": "\u200a{{ if not .Error }}{{ .Icon }} <f>{{ .Percentage }}\uf295</f>{{ end }}{{ .Error }}\u200a",
          "powerline_symbol": "<r>\ue0b2</r>",
          "style": "powerline",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:warning-red{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:dark-color{{end}}",
            "{{if eq \"Full\" .State.String}}p:dark-color{{end}}"
          ],
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:light-color{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:warning-red{{end}}",
            "{{if eq \"Full\" .State.String}}p:light-color{{end}}"
          ]
        },
        {
          "type": "sysinfo",
          "alias": "memory_usage_percent",
          "properties": {
            "cache_duration": "none",
            "precision": 2
          },
          "template": "\uf85a {{ round .PhysicalPercentUsed .Precision }}\uf295\u200a",
          "foreground": "p:dark-color",
          "powerline_symbol": "<r>\ue0b2</r>",
          "background": "p:light-color",
          "style": "powerline"
        },
        {
          "type": "connection",
          "alias": "wifi_ethernet_cellular_connection",
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if eq .Type \"wifi\"}}\uf1eb{{ else if eq .Type \"ethernet\"}}\uf6ff{{ else if eq .Type \"cellular\" }}\uf61d{{ end }}\u200a",
          "foreground": "parentForeground",
          "background": "parentBackground",
          "style": "powerline"
        },
        {
          "type": "time",
          "alias": "current_time_with_clock",
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u200a\uf017\u200a{{ .CurrentDate | date .Format }}",
          "foreground": "p:light-color",
          "powerline_symbol": "<r>\ue0b2</r>",
          "background": "p:dark-color",
          "style": "powerline"
        },
        {
          "type": "time",
          "alias": "beauty_end_line_sun_moon",
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "{{if or (gt (now).Hour 18) (lt (now).Hour 6)}}<p:moon-light>\uf186</>{{ else }}<p:sun-light>\ufaa7</>{{ end }}",
          "foreground": "parentForeground",
          "background": "parentBackground",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alias": "left_prompt_decorations",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{if eq .Var.ShowPromptLineCount \"yes\"}}<p:light-color>{{.PromptCount}}</>\u200a{{end}}\u2500‚ùØ",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Root }}p:warning-red{{ else }}p:light-color{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}